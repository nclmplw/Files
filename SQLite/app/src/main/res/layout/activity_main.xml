<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="234dp"
        android:layout_y="450dp"
        android:text="QUANTITY" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="175dp"
        android:layout_y="380dp"
        android:text="ITEM NAME" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="2dp"
        android:layout_y="449dp"
        android:text="ITEM PRICE" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="125dp"
        android:layout_y="27dp"
        android:text="ANYTHING STORE"
        android:textColor="#029A0D"
        android:textSize="23dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="4dp"
        android:layout_y="173dp"
        android:text="FIRSTNAME" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="2dp"
        android:layout_y="240dp"
        android:text="MIDDLENAME" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="3dp"
        android:layout_y="310dp"
        android:text="LASTNAME" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="2dp"
        android:layout_y="95dp"
        android:text="ORDER DATE" />

    <Button
        android:id="@+id/totalpay"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_x="2dp"
        android:layout_y="528dp"
        android:text="TOTAL PAYABLE" />

    <EditText
        android:id="@+id/petsav"
        android:layout_width="315dp"
        android:layout_height="wrap_content"
        android:layout_x="95dp"
        android:layout_y="84dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:gravity="center"
        />

    <EditText
        android:id="@+id/nganv"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_x="2dp"
        android:layout_y="192dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/mnganv"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="259dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/apilyedov"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="332dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/itemnamev"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:layout_x="-2dp"
        android:layout_y="399dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:gravity="center"
        />

    <EditText
        android:id="@+id/presyov"
        android:layout_width="229dp"
        android:layout_height="wrap_content"
        android:layout_x="1dp"
        android:layout_y="475dp"
        android:ems="10"
        android:inputType="number" />

    <EditText
        android:id="@+id/pilav"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:layout_x="234dp"
        android:layout_y="475dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="number" />

    <TextView
        android:id="@+id/totalpayable"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_x="0dp"
        android:layout_y="567dp"
        android:gravity="center"
        android:text="" />

    <Button
        android:id="@+id/update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="626dp"
        android:text="INSERT" />

    <Button
        android:id="@+id/delete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="673dp"
        android:text="VIEW RECORDS" />

</AbsoluteLayout>